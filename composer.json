{
    "name": "jordanpartridge/conduit",
    "description": "Your personal developer API & MCP integration engine - AI-ready GitHub CLI and beyond",
    "keywords": ["github", "cli", "mcp", "ai", "laravel-zero", "developer-tools", "integration"],
    "homepage": "https://github.com/jordanpartridge/conduit",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Partridge",
            "email": "jordan.l.partridge@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "guzzlehttp/guzzle": "^7.8",
        "laravel-zero/framework": "^11.36.1",
        "symfony/process": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18.3",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["conduit"]
}
